{
  "name": "kipas-angin/response-handler",
  "description": "A standarized response handler that made for you, easy to use and ready to go",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Tariq Roja Abdullah",
      "email": "tariqrojaabdullah@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/support": "^12.22",
    "illuminate/http": "^12.22"
  },
  "autoload": {
    "psr-4": {
      "KipasAngin\\ResponseHandler\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "KipasAngin\\ResponseHandler\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "KipasAngin\\ResponseHandler\\ResponseHandlerServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "phpunit/phpunit": "^12.3",
    "orchestra/testbench": "^10.4",
    "mockery/mockery": "^1.6"
  }
}
